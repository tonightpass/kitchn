{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**/*.d.ts",
        "dist/**/*.mjs",
        "dist/**/*.js",
        "dist/**/*.css",
        ".next/**",
        "index.js",
        "ssg.js",
        "loader.js"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}
